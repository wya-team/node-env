  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./config/commit-lint"
    }
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,vue}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax scss",
      "git add"
    ]
  }
